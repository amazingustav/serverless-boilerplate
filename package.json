{
  "name": "serverless-boilerplate",
  "version": "1.4.0",
  "description": "Serverless boilerplate service",
  "main": "serverless.ts",
  "scripts": {
    "clean": "rimraf coverage .eslintcache dist node_modules lib",
    "build:clean": "rimraf dist",
    "build:code": "tsc",
    "build": "run-s build:clean build:code",
    "format": "prettier --cache --write \"src/**/*.ts\"",
    "lint": "eslint --cache --cache-strategy content --report-unused-disable-directives .",
    "prepare": "npm run build",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:production": "serverless deploy --stage production",
    "test": "vitest run",
    "test:update-snapshots": "vitest run -u",
    "coverage": "vitest run --coverage",
    "bootstrap": "rm -rf node_modules && yarn install",
    "start:staging": "yarn serverless offline --stage staging",
    "start:production": "yarn serverless offline --stage production",
    "swagger": "swagger-ui-watcher ./openapi/api-spec.yml"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "dependencies": {
    "@dazn/lambda-powertools-logger": "^1.28.1",
    "@middy/core": "^4.5.1",
    "@middy/http-json-body-parser": "^4.6.4",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1397.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.7.0",
    "@middy/http-header-normalizer": "^4.6.4",
    "@serverless/typescript": "^3.30.1",
    "@swc/core": "^1.3.64",
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "c8": "^8.0.1",
    "commitlint": "^17.6.5",
    "esbuild": "^0.18.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "8.0.3",
    "json-schema-to-ts": "^2.9.1",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "rimraf": "^5.0.5",
    "serverless": "^3.35.2",
    "serverless-domain-manager": "^7.0.4",
    "serverless-esbuild": "^1.45.1",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-aws-alerts": "^1.7.5",
    "serverless-plugin-canary-deployments": "^0.8.0",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-prune-plugin": "^2.0.2",
    "serverless-stage-manager": "^1.0.5",
    "swagger-ui-watcher": "^2.1.13",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "author": "Blizz",
  "license": "MIT"
}
